function checkProbabilityTheory(count) {
  let evenCount = 0;
  let oddCount = 0;
  for (let i = 0; i < count; i++) { 
       let randomNumber = Math.floor(Math.random() * (1000 - 100 + 1)) + 100;
       if (randomNumber % 2 === 0) {
           even++;
      } else {
         odd++;
      }
  } 
  return `Кількість парних: ${even}, Кількість непарних: ${odd}`;
} 
